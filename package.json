{
  "name": "growth-compass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "node migrations/migrate.js",
    "migrate:create": "touch migrations/$(date +%Y%m%d%H%M%S)_$1.sql",
    "migrate:up": "tsx src/lib/database/migration-runner.ts up",
    "migrate:down": "tsx src/lib/database/migration-runner.ts down",
    "migrate:status": "tsx src/lib/database/migration-runner.ts status",
    "migrate:validate": "tsx src/lib/database/migration-runner.ts validate",
    "migrate:reset": "tsx src/lib/database/migration-runner.ts reset",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "workers": "node scripts/start-workers.js",
    "workers:dev": "nodemon scripts/start-workers.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run workers:dev\"",
    "analyze": "ANALYZE=true npm run build",
    "build:optimized": "cp next.config.optimization.ts next.config.ts && npm run build",
    "generate:sessions": "tsx scripts/generate-weekly-sessions.ts"
  },
  "dependencies": {
